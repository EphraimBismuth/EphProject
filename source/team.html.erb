<div id="team">
  <div class="team-content">
    <h1>This template was built with <i class="fa fa-heart" aria-hidden="true"></i>
 by</h1>
  <ul class="list-inline">
    <% data.users.each do |user| %>
      <li>
        <% link_to "https://kitt.lewagon.com/alumni/#{user.github_nickname}", target: "_blank" do %>
          <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{user.github_nickname}", class: "img-circle" %>
          <% end %>
          <h2><%= user.name %></h2>
          <i class="fab fa-github"></i><p><%= user.github_nickname %></p>
      </li>
      <% end %>
  </ul>
  </div>
</div>
